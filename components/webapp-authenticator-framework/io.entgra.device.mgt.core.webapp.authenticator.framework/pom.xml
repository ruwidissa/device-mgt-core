                            <!--javax.xml.bind.annotation;version="[2.3,3)",-->
                            javax.xml.parsers;version="${javax.xml.parsers.import.pkg.version}",
                            <!--javax.xml.stream;version="[1.0,2)",-->
                            javax.xml.stream.*;version="[1.0.1,1.1.0)",
                            org.apache.axiom.om;version="[1.2,2)",
                            org.apache.axiom.om.impl.builder;version="[1.2,2)",
                            org.apache.axiom.om.util;version="[1.2,2)",
